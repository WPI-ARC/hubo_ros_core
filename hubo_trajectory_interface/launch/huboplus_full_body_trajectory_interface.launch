<launch>

  <arg name="output" default="screen" />
  <arg name="robot" default="huboplus" />

  <include file="$(find hubo_trajectory_interface)/launch/full_body_trajectory_controller_node.launch" >
    <arg name="output" value="$(arg output)" />
    <arg name="robot" value="$(arg robot)" />
  </include>

  <include file="$(find hubo_trajectory_interface)/launch/full_body_trajectory_action_node.launch" >
    <arg name="output" value="$(arg output)" />
    <arg name="robot" value="$(arg robot)" />
  </include>

</launch>

