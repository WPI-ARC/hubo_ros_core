<launch>

  <arg name="output" default="screen" />
  <arg name="robot" default="drchubo" />

  <group ns="$(arg robot)_fullbody_controller">

    <node name="$(arg robot)_fullbody_controller_node" pkg="hubo_trajectory_interface" type="hubo_trajectory_compromise_interface" output="$(arg output)" >
        <rosparam command="load" file="$(find hubo_components)/config/$(arg robot)_fullbody_params.yaml" />
    </node>

  </group>

</launch>

